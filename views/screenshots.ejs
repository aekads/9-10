<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Screenshots</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      padding: 8px 12px;
      border: 1px solid #ddd;
    }
    th {
      background-color: #f4f4f4;
    }
  </style>
</head>
<body>
  <h1>Screenshot Data</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Type</th>
        <th>Filename</th>
        <th>Image URL</th>
        <th>Size</th>
      </tr>
    </thead>
    <tbody>
      <% if (screenshots.length > 0) { %>
        <% screenshots.forEach((screenshot) => { %>
          <tr>
            <td><%= screenshot.id %></td>
            <td><%= screenshot.type %></td>
            <td><%= screenshot.filename %></td>
            <td><a href="<%= screenshot.image_url %>" target="_blank">View Image</a></td>
            <td><%= screenshot.size %> bytes</td>
              <button onclick="Dhvanil('<%= clientId %>')">Dhvanil</button>
                <button onclick="rbt('<%= clientId %>')">sakirb</button>

          </tr>
        <% }); %>
      <% } else { %>
        <tr>
          <td colspan="5">No data available</td>
        </tr>
      <% } %>
    </tbody>
  </table>





  <script>
  

    function Dhvanil(clientId) {
      fetch(`/Dhvanil/${clientId}`, { method: 'POST' })
        .then(response => response.json())
        .then(data => alert(data.message))
        .catch(error => console.error('Error:', error));
    }








    function rbt(clientId) {
      fetch(`/rbt/${clientId}`, { method: 'POST' })
        .then(response => response.json())
        .then(data => alert(data.message))
        .catch(error => console.error('Error:', error));
    }












































    
  </script>
</body>
</html>
